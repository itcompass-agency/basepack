{
    "name": "itcompass/basepack",
    "description": "DevOps toolkit for Laravel projects with Docker, Make commands, and development environment setup",
    "keywords": ["laravel", "docker", "devops", "development", "make", "php", "nginx", "mysql"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/itcompass-agency/basepack",
    "authors": [
        {
            "name": "ITCompass Agency",
            "email": "info@itcompass.agency"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ITCompass\\BasePack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ITCompass\\BasePack\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ITCompass\\BasePack\\BasePackServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}